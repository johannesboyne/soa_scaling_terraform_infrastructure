[
  {
    "name": "dbtask",
    "image": "johannesboyne/terraform_example_db:0.1",
    "cpu": 120,
    "memory": 90,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 1337,
        "hostPort": 81
      }
    ],
    "mountPoints": [
      {
        "sourceVolume": "persistend_data",
        "containerPath": "/data/db",
        "readOnly": false
      }
    ],
    "environment" : [
      { "name" : "PORT", "value" : "1337" }
    ],
    "command": []
  }
]
